<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CRMBundle\Entity\Mark" table="mark">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mark_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="name" type="string" column="name" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="class" type="string" column="class" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="model" type="string" column="model" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="places" type="integer" column="places" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="yearOfIssue" type="integer" column="year_of_issue" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="engineCapacity" type="float" column="engine_capacity" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
