<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CRMBundle\Entity\Car" table="car">
    <indexes>
      <index name="idx_773de69d4290f12b" columns="mark_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="car_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="number" type="string" column="number" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="mark" target-entity="Mark" fetch="LAZY">
